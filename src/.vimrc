set nobackup
set noswapfile
set viminfo=
set autoread
set hidden
set showcmd

set number
set virtualedit=onemore
set smartindent
set visualbell
set showmatch
set laststatus=2
set wildmode=list:longest
nnoremap j gj
nnoremap k gk
syntax enable

set list listchars=tab:\\t
set expandtab
set tabstop=4
set shiftwidth=4
set nofixeol
