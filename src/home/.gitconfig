[user]
	name = "Ryota Kobayashi"
	email = "38117745+kobar9568@users.noreply.github.com"
[color]
	ui = auto
[core]
	autoCRLF = false
	pager = "LESSCHARSET=utf-8 less"
[pull]
	rebase = false
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[init]
	defaultBranch = master
[merge]
	tool = vimdiff
	conflictstyle = diff3
[mergetool]
	prompt = false
[safe]
	directory = C:/Scoop/apps/scoop/current
	directory = C:/Scoop/buckets/main
[credential]
	helper = manager-core # If you have installed Git for Windows with scoop, "git config --system --unset credential.helper" is required.
